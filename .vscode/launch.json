{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Dev Server",
        "type": "node",
        "request": "launch",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/vite",  // 如果你使用 Vite
        "args": ["dev"],
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal"
      }
    ]
  }
  